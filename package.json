{
  "name": "bean-vibes-api",
  "version": "0.1.0",
  "description": "Create a Node.js app for building production-ready RESTful APIs using Express, by running one command",
  "bin": "bin/createNodejsApp.js",
  "main": "src/index.js",
  "repository": "https://github.com/hagopj13/node-express-boilerplate.git",
  "author": "Hagop Jamkojian <hagopj13@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "seed:categories": "NODE_ENV=development node scripts/seedCategories.js",
    "seed:places": "NODE_ENV=development node scripts/seedPlaces.js",
    "seed:users": "NODE_ENV=development node scripts/seedUsers.js",
    "seed:restricted-words": "NODE_ENV=development node scripts/seedRestrictedWords.js",
    "reset:restricted-words": "NODE_ENV=development node scripts/resetRestrictedWords.js",
    "seed:districts-wards": "NODE_ENV=development node scripts/seedDistrictsAndWards.js",
    "reset:districts-wards": "NODE_ENV=development node scripts/resetAndSeedDistrictsWards.js",
    "stats:districts-wards": "NODE_ENV=development node scripts/statsDistrictsWards.js",
    "seed:categories-from-districts": "NODE_ENV=development node scripts/seedCategoriesFromDistricts.js",
    "reset:categories-from-districts": "NODE_ENV=development node scripts/seedCategoriesFromDistricts.js --reset",
    "stats:categories": "NODE_ENV=development node scripts/statsCategories.js",
    "reset:weekly-stats": "NODE_ENV=development node scripts/resetWeeklyStats.js",
    "update:hot-scores": "NODE_ENV=development node scripts/updateHotScores.js",
    "update:place-ratings": "NODE_ENV=development node scripts/updateAllPlaceRatings.js",
    "remove:last-viewed-at": "NODE_ENV=development node scripts/removeLastViewedAt.js",
    "seed": "node scripts/seed.js",
    "reset-db": "node scripts/reset-db.js",
    "update:places": "NODE_ENV=development node scripts/updatePlaces.js",
    "add:places-random-categories": "NODE_ENV=development node scripts/addPlaceWithRandomCategories.js",
    "add:places-random-categories:sample": "NODE_ENV=development node scripts/addPlaceWithRandomCategories.js --sample",
    "add:places-random-categories:single": "NODE_ENV=development node scripts/addPlaceWithRandomCategories.js --single",
    "prepare": "husky"
  },
  "keywords": [
    "node",
    "node.js",
    "boilerplate",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-xss-sanitizer": "^2.0.0",
    "helmet": "^8.1.0",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.16.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^6.0.8",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^2.1.1",
    "faker": "^6.6.6",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "node-mocks-http": "^1.13.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0"
  }
}
